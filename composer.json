{
    "name": "wildfire/tribe",
    "description": "a web project management system by wildfire.",
    "repositories": {
        "wildfire/backup": {
            "type": "git",
            "url": "https://github.com/wil-ldf-ire/backup.git"
        },
        "wildfire/sitemap": {
            "type": "git",
            "url": "https://github.com/wil-ldf-ire/sitemap.git"
        },
        "wildfire/admin": {
            "type": "git",
            "url": "https://github.com/wil-ldf-ire/admin.git"
        },
        "wildfire/auth": {
            "type": "git",
            "url": "https://github.com/wil-ldf-ire/auth.git"
        },
        "wildfire/core": {
            "type": "git",
            "url": "https://github.com/wil-ldf-ire/core.git"
        },
        "tribe/core": {
            "type": "git",
            "url": "https://github.com/cityowltypes/def-tribe-core.git"
        },
        "wildfire/api": {
            "type": "git",
            "url": "https://github.com/wil-ldf-ire/api.git"
        }
    },
    "require": {
        "php": "^7.4",
        "vlucas/phpdotenv": "^5.2",
        "wildfire/core": "^3.0",
        "wildfire/auth": "^2.0",
        "wildfire/admin": "^2.0",
        "wildfire/api": "^0.1",
        "wildfire/backup": "^1.1",
        "wildfire/sitemap": "^1.1",
        "botman/botman": "^2.6",
        "botman/driver-telegram": "^1.6",
        "landrok/language-detector": "^1.3",
        "ext-json": "*",
        "ext-mysqli": "*",
        "friendsofphp/php-cs-fixer": "^3.10",
        "tribe/core": "^4.0",
        "tribe/auth": "^3.0",
        "tribe/api": "^1.0",
        "tribe/uploads": "^1.0",
        "tribe/backup": "^2.0",
        "mikehaertl/php-pdftk": "^0.13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Theme\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4.33"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}